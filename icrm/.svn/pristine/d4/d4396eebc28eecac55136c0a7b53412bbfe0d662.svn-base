package com.zjhcsoft.icrm.dynamic.page.web.form;

public class FieltSet {

	
	public String getHtml(String innerHtml){
		StringBuffer htmlBf = new StringBuffer();
		htmlBf.append("<fieldset>");
		htmlBf.append("<legend>");
		htmlBf.append(innerHtml);
		htmlBf.append("</legend>");
		htmlBf.append("</fieldset>");
		return htmlBf.toString();
	}
	
}
