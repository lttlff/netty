package com.zjhcsoft.icrm.dynamic;

/**
 * 
 * @author zhunb
 * @date 2014-2-11 页面展示用jsonBean
 */
public class WebJsonBean {

	private String name;// 字段名称
	private String label;// 字段Label
	private Object value;// 值 可能是String 或是JSONObject JSONArray

	private String type;// 类型 text combo date textarea trigger fieldset tab

	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

	public Object getValue() {
		return value;
	}

	public void setValue(Object value) {
		this.value = value;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

}
